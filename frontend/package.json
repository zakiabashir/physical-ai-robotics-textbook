{
  "name": "physical-ai-robotics-textbook",
  "version": "0.1.0",
  "private": true,
  "description": "Physical AI & Humanoid Robotics - An interactive textbook",
  "keywords": [
    "robotics",
    "physical-ai",
    "ros2",
    "gazebo",
    "isaac",
    "textbook",
    "education"
  ],
  "homepage": "https://physical-ai-robotics.org",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:vercel": "echo '{}' > localStorage.json && cross-env NODE_OPTIONS=\"--localstorage-file=./localStorage.json --max-old-space-size=4096\" docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "lint": "eslint src docs --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src docs --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src docs/**/*.{js,jsx,ts,tsx,md,mdx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "backend:dev": "cd backend && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "backend:prod": "cd backend && uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "dev": "concurrently \"npm run start\" \"npm run backend:dev\"",
    "validate-content": "node scripts/validate-content.js",
    "deploy:frontend": "npm run build && gh-pages -d build",
    "deploy:backend": "cd backend && railway up",
    "deploy:all": "chmod +x scripts/deploy.sh && ./scripts/deploy.sh all",
    "deploy:prod": "scripts\\deploy.bat all",
    "build:prod": "npm run build && npm run test"
  },
  "dependencies": {
    "@docusaurus/core": "^3.0.0",
    "@docusaurus/plugin-content-docs": "^3.0.0",
    "@docusaurus/plugin-ideal-image": "^3.0.0",
    "@docusaurus/preset-classic": "^3.0.0",
    "@docusaurus/theme-mermaid": "^3.0.0",
    "@docusaurus/theme-search-algolia": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@monaco-editor/react": "^4.6.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "mermaid": "^10.6.1",
    "prism-react-renderer": "^2.1.0",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^4.4.0",
    "react-markdown": "^9.0.0",
    "react-router-dom": "^6.18.0",
    "react-select": "^5.8.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "styled-components": "^6.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.0.0",
    "@docusaurus/types": "^3.0.0",
    "@tsconfig/docusaurus": "^2.0.0",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@types/react-syntax-highlighter": "^15.5.9",
    "@types/styled-components": "^5.1.29",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
